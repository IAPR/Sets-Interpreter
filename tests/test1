Set C1 := { A, B, C, D, E };
Set C2 := { F, A, B, C, G };
Set C3 := { C, F, H, A, J };
Set C4 := { A, E, G, L, M };
Sets;
ShowSet C4;
ShowSet C3;
ShowSet C2;
ShowSet C1;
Union C1, C2, C3, C4;
Intersect C1, C2, C3, C4;
SetUnion C7, C1, C2, C3, C4;
SetIntersect C8, C1, C2, C3, C4;
ShowSets;
SetUnion C1, C2, C3, C4;
SetIntersect C7, C1, C2, C3, C4;
ShowSets;
C5<-C1;
C6<-C2;
Sets;
ShowSets;
Delete C1;
Delete C2;
ShowSets;
Delete C3;
Delete C4;
ShowSets;
Delete C5;
Delete C6;
Sets;
